Private Sub Workbook_Open()
  Set ctlEvent = New clsEvent
  Set ctlEvent.ExcelApplication = Application
  ctlEvent.InitializeBookSheets
  ctlEvent.StopEvent = False



  Call Application.OnKey("{F1}", "")
End Sub



