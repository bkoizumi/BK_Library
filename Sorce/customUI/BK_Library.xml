<?xml version="1.0" encoding="UTF-8" ?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui"  onLoad="BK_Library.xlam!Ctl_Ribbon.onLoad">
   <ribbon startFromScratch="false">
 <tabs>
      <tab idMso="TabView">
        <group id="dispGroup" label="BK_Libraryタブ表示">
          <checkBox id="BK_Library"  label="BK_Library"  onAction="BK_Library.xlam!Ctl_Ribbon.setDispTab"  />
        </group>
      </tab>


    <tab id="BK_LibraryTab" label="カスタムメニュー" insertBeforeMso="TabHome" getVisible="BK_Library.xlam!Ctl_Ribbon.getVisible">
    <group  id="カスタマイズ" label="カスタマイズ">
      <button id="Option" label="Option" imageMso="AnimationCustomAddActionVerbDialog" size="large" screentip="オプション設定"  supertip="オプション画面を表示します &#13;" onAction="BK_Library.xlam!Main.オプション画面表示" />

      <splitButton id="splitBtn01" size="large">
        <button id="Favorite_detail" label="お気に入り&#xD;&#xA;" imageMso="Favorites" screentip="お気に入り" supertip="お気に入り登録リスト表示" onAction="BK_Library.xlam!Ctl_Favorite.detail" />
          <menu id="Favorite_Menu" screentip="お気に入りメニュー"  supertip="お気に入りメニュー表示" itemSize="large">
            <dynamicMenu  id="Favorite_List"  imageMso="Favorites"  invalidateContentOnDrop="true" label="お気に入り一覧"  getContent="BK_Library.xlam!Ctl_Ribbon.FavoriteMenu" />
          </menu>
      </splitButton>

      <separator id="Sprt01" />

      <button id="DefaultDisplay" label="標準画面" imageMso="ViewNormalViewExcel" size="normal" screentip="標準画面"  supertip="標準画面" onAction="BK_Library.xlam!Main.標準画面" />
      <menu id="CustomMenu" label="メニュー"  itemSize="large" imageMso="BlogHomePage">
        <button id="Invisible_RibbonTab"  label="リボンタブ非表示"  description="リボンタブ非表示"      imageMso="AppointmentColor8"      onAction="BK_Library.xlam!Main.A" />
        <button id="del_Style"            label="スタイル削除"      description="スタイルを削除します"  imageMso="AppointmentColorDialog" onAction="BK_Library.xlam!Main.スタイル削除" />
        <button id="del_CellNames"        label="名前定義削除"      description="名前定義削除"          image="del_CellNames"             onAction="BK_Library.xlam!Main.名前定義削除" />
        <button id="set_Images"           label="画像設定"          description="画像設定"              imageMso="ControlImage"           onAction="BK_Library.xlam!Main.画像設定" />
      </menu>

      <button id="HighLight"      label="ハイライト"  image="HighLight" size="normal"  screentip="ハイライト"  supertip="ハイライト" onAction="BK_Library.xlam!Main.ハイライト" />
      <button id="Notation_R1C1"  label="R1C1表記"    image="dispR1C1"  size="normal"  screentip="R1C1表記"    supertip="R1C1参照形式に変更"   onAction="BK_Library.xlam!Main.R1C1表記" />

      <menu id="LineMenu" label="罫線" itemSize="large"  imageMso="BorderDrawGrid">
        <button id="Line_Clear"  label="クリア"  description="罫線を削除します"  imageMso="BorderErase"      onAction="BK_Library.xlam!Main.罫線_削除" />

        <menuSeparator id="囲み" title="囲み" />
        <button id="Line_Table"  label="表"  description="表形式の罫線を設定します &#13;"  imageMso="BorderDrawGrid"      onAction="BK_Library.xlam!Main.罫線_表" />

        <menuSeparator id="破線" title="破線" />
        <button id="Line_破線_水平"  label="水平"  description="破線で水平方向に罫線を設定します &#13;"  imageMso="BorderInsideHorizontal"    onAction="BK_Library.xlam!Main.罫線_破線_水平" />
        <button id="Line_破線_垂直"  label="垂直"  description="破線で垂直方向に罫線を設定します &#13;"  imageMso="BorderInsideVertical"      onAction="BK_Library.xlam!Main.罫線_破線_垂直" />
        <button id="Line_破線_左右"  label="左右"  description="破線で左右方向に罫線を設定します &#13;"  image="BorderLeftAndRight"           onAction="BK_Library.xlam!Main.罫線_破線_左右" />
        <button id="Line_破線_上下"  label="上下"  description="破線で上下方向に罫線を設定します &#13;"  imageMso="BordersAndShadingInfoPath" onAction="BK_Library.xlam!Main.罫線_破線_上下" />
        <button id="Line_破線_囲み"  label="囲み"  description="破線で囲み方向に罫線を設定します &#13;"  imageMso="BorderOutside"             onAction="BK_Library.xlam!Main.罫線_破線_囲み" />
        <button id="Line_破線_格子"  label="格子"  description="破線で格子方向に罫線を設定します &#13;"  imageMso="BordersAll"                onAction="BK_Library.xlam!Main.罫線_破線_格子" />

        <menuSeparator id="二重線" title="二重線" />
        <button id="Line_左右"  label="左右"  description="二重線で左右に罫線を設定します &#13;"  image="BorderDoubleLeftAndRight" onAction="BK_Library.xlam!Main.罫線_二重線_左右" />
        <button id="Line_上下"  label="上下"  description="二重線で上下に罫線を設定します &#13;"  image="BorderDoubleTopAndBottom" onAction="BK_Library.xlam!Main.罫線_二重線_上下" />
        <button id="Line_囲み"  label="囲み"  description="二重線で囲みに罫線を設定します &#13;"  image="BorderDouble"             onAction="BK_Library.xlam!Main.罫線_二重線_囲み" />
      </menu>

      <dynamicMenu id="SheetList" label="シート一覧" imageMso="HeaderFooterSheetNameInsert" size="normal" getContent="BK_Library.xlam!Ctl_Ribbon.getSheetsList" />

      <menu id="dummyData" label="dummyData"  imageMso="ResetToTemplate">
        <button id="makeDummyData"  label="生成"  description="ダミーデータを生成します"  imageMso="ResetToTemplate"      onAction="BK_Library.xlam!Main.A" />
      </menu>
    </group >



      <group id="Maintenance" label="メンテナンス">
        <button id="SheetList_Refresh"  label="リボン更新"  imageMso="DataRefreshAll" size="normal" screentip="リボン更新" supertip="リボン更新を更新します;"  onAction="BK_Library.xlam!Ctl_Ribbon.dMenuRefresh" />
