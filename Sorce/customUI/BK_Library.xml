        <control idMso="RemoveDuplicates" showLabel="false"     visible="true" />
      </box>
      <control idMso="Filter"     size="large"      showLabel="false"     visible="true" />
    </group>
    <group idMso="GroupOutline"                    visible="true"/>                               <!--アウトラインー-->
